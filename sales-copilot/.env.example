# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# HubSpot
HUBSPOT_CLIENT_ID=your_hubspot_client_id
HUBSPOT_CLIENT_SECRET=your_hubspot_client_secret
HUBSPOT_REDIRECT_URI=https://your-project.supabase.co/functions/v1/hubspot-oauth
HUBSPOT_SCOPE="crm.objects.contacts.write crm.objects.companies.write crm.objects.deals.write crm.schemas.contacts.write crm.schemas.companies.write crm.schemas.deals.write"
HUBSPOT_APP_ID=your_hubspot_app_id

# Encryption
ENCRYPTION_KEY=your_32_byte_encryption_key

# AI Provider API Keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_API_KEY=your_google_api_key

# Application Configuration
APP_URL=http://localhost:3000
APP_ENV=development
APP_INSTALL_SUCCESS_URI=http://localhost:3000/install/success
APP_INSTALL_FAILED_URI=http://localhost:3000/install/failed

# Pinecone Configuration
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment