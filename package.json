{
  "name": "nsw-jobs-etl",
  "version": "1.0.0",
  "description": "ETL process for NSW Government jobs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "cross-env NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" ts-node src/index.ts",
    "dev": "ts-node-esm src/index.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "inquirer": "^9.2.14",
    "openai": "^4.26.0",
    "ora": "^8.0.1",
    "pdf2json": "^3.1.6",
    "puppeteer": "^21.9.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
